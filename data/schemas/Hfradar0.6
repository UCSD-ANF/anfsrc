	Null ( "-9999.0" )
	Description ( "monopole cross spectra noise floor" )
	Detail {
		This is the cross spectra noise floor of monopole
	}
	;

Attribute mono_css_snr
	Real (15)
	Format ( "%15.6f" )
	Null ( "-9999.0" )
	Description ( "monopole cross spectra SNR" )
	Detail {
		This is the cross spectra signal to noise of monopole
	}
	;

Attribute mtime
	Time (17)
	Format ( "%17.5f" )
	Null ( "-9999999.99999" )
	Units ( "seconds" )
	Description ( "file modification time" )
	Detail {
		This attribute records the time of last file modification, 
		according to the filesystem.
	}
	;

Attribute music_param_01
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 1" )
	;

Attribute music_param_02
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 2" )
	;

Attribute music_param_03
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 3" )
	;

Attribute net
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "unique network identifier" )
