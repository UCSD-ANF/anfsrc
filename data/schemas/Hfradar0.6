	Description ( "version of radialSlider program" )
	;

Attribute rad_type
	Integer (10) 
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "rad_type >= 0" )
	Description ( "type of radial" )
	Detail {
		This field gives the type of radial being processed. 
	}
	;

Attribute rad_vect_count
	Integer (10) 
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "rad_vect_count >= 0" )
	Description ( "number of radial vector solutions" )
	Detail {
		This is the total number of radial vectors solutions found 
