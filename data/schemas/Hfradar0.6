Attribute rad_bragg_noise_thr
	Real (12)
	Format ( "%12.8f" )
	Range ( "rad_bragg_noise_thr >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	;

