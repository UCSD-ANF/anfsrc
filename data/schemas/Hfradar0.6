		This character string is the name of a radar network.
	}
	;

Attribute netname
	String (80)
	Format ( "%-80s" )
	Null ( "-" )
	Description ( "network name" )
	Detail {
		String containing the name of a network."
	}
	;

Attribute nmerge_rads
	Integer (6)
	Format ( "%6d" )
	Null ( "-1" )
	Range ( "nmerge_rads >= 0" )
	Description ( "number of short time radials merged into radial file" )
	;

Attribute npll_unlock
	Integer (10)
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "npll_unlock >= 0" ) 
	Description ( "number of PLL lock losses" )
	Detail {
		This is the number of times the receiver PLL was 
