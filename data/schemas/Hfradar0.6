Schema Hfradar0.6
	Description ( "Prototype schema for HFRADAR data" )
	Detail {
		This schema holds intermediate and processed 
		measurements for Doppler-radar current measurements. 
		Support is included to track the raw files of 
		radial HFRADAR information.
	}
	Timedate lddate
	;

Attribute algorithm
	String (15)
	Format ( "%-15s" )
	Null ( "-" )
	Description ( "processing algorithm used" )
	Detail {
		This is a brief code string identifying the algorithm 
		used for processing.
	}
	;

Attribute auth
	String (15)
	Format ( "%-15s" )
	Null ( "-" )
	Description ( "source/originator of data" )
	Detail {
		This records the originator of a piece of processed 
		data, e.g. a total vector file. Note that this field 
		should be used only to identify the author, not to 
		imply a certain processing algorithm, which should be 
		separately reflected in the algorithm field.
	}
	;

Attribute avg_rad_bearing
	Real (15)
	Format ( "%15.6f" )
	Null ( "-9999.0" )
	Description ( "average radial bearing" )
	Detail {
		This is the averaged bearing of all radial vectors.
	}
	;

Attribute avg_rad_spd
	Real (15)
	Format ( "%15.6f" )
	Null ( "-9999.0" )
	Range ( "avg_rad_spd >= 0" )
	Description ( "average current speed" )
	Detail {
		This is the averaged absolute velocity (always positive).
	}
	;

Attribute avg_rads_per_range
	Integer (10) 
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "avg_rads_per_range >= 0" )
	Description ( "average # of radial vectors per range cell" )
	Detail {
		This is the average number of radial vectors per range cell
	}
	;

Attribute awg_run_time
	Integer (17)
	Format ( "%17d" )
	Null ( "-9999" )
	Range ( "awg_run_time >= 0" ) 
	Description ( "receiver run time" )
	Detail {
		This is the receiver runtime since it was last
		powered or the AWG module restarted
	}
	;

Attribute awg_tmp
	Integer (10)
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "awg_tmp >= 0" ) 
	Description ( "AWGIII model temperature" )
	Detail {
		This is the receiver AWGIII model temperature
	}
	;

Attribute bragg_smooth_pts
	Integer (4)
	Format ( "%4d" )
	Range ( "bragg_smooth_pts >= 0" )
	Null ( "-1" )
	Description ( "Number of smoothing points applied to cross spectra" )
	Detail {
		This is the number of smoothing points applied to cross 
		spectra for first-order region determination
	}
	;

Attribute cfreq
	Real (15)
	Format ( "%15.11f" )
	Null ( "-89999999999999" )
	Units ( "MHz" )
	Description ( "Center Frequency of a radar installation" )
	Detail {
		This field lists the center-frequency in MHz of a
		radar installation.
	}
	;

Attribute codartools_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of codartools.pm Perl module used" )
	;

Attribute cpu_runtime
	Real (17)
	Format ( "%17.5f" )
	Null ( "-9999.0" )
	Range ( "cpu_runtime >= 0" ) 
	Description ( "Computer run-time counter" )
	Detail {
		This is the CPU run-time counter in minutes
	}
	;

Attribute ctf_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "CODAR/columnar table format version" )
	;

Attribute curr_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of currents tools used in OS9" )
	;

Attribute dfile
	String (64)
	Format ( "%-64s" )
	Null ( "-" )
	Description ( "data file" )
	Detail {
		This attribute is the file name of a disk-based 
		external file. See dir.
	}
	;

Attribute diag_range_cell
	Integer (10) 
	Format ( "%10d" )
	Null ( "-9999" )
	Range ( "diag_range_cell >= 0" )
	Description ( "highest recorded SSN3" )
	Detail {
		This is the cross spectra range cell which had the 
		highest SSN3 for which the noise floor and signal to 
		noise were recorded
	}
	;

Attribute dir
	String (128)
	Format ( "%-128s" )
	Null ( "-" )
	Description ( "directory" )
	Detail {
		This attribute is the directory-part of a path name.
		Relative path names or '.' (dot), the notation for the
		current directory, may be used.
	}
	;

Attribute dres
	Real (12) 
	Format ( "%12.8f" )
	Range ( "dres >= 0" )
	Null ( "-9999.0" )
	Description ( "doppler cell spacing of the cross spectra" )
	;

Attribute dual_angle_pcnt
	Integer (6) 
	Format ( "%6d" )
	Null ( "-9999" )
	Range ( "dual_angle_pcnt >= 0" )
	Description ( "percentage of doppler cells with dual angle solutions" )
	Detail {
		This is the percentage of doppler cells that had dual angle
		MUSIC solutions
	}
	;

Attribute earth_radius
	Real (21)
	Format ( "%21.15f" )
	Range ( "earth_radius >= 0" )
	Null ( "-9999.0" )
	Description ( "equatorial earth radius" )
	;

Attribute ellip
	String (50)
	Format ( "%-50s" )
	Null ( "-" )
	Description ( "ellipsoid model used to represent earth's surface" )
	;

Attribute ellip_flatten
	Real (24)
	Format ( "%24.18f" )
	Range ( "ellip_flatten >= 0" )
	Null ( "-9999.0" )
	Description ( "ellipsoid flattening term" )
	;

Attribute endtime
	Time (17)
	Format ( "%17.5f" )
	Null ( "9999999999.99900" )
	Units ( "seconds" )
	Description ( "end of a time range" )
	Detail {
		This is the time a site-table entry expires.
	}
	;

Attribute first_order_calc
	Integer (6)
	Format ( "%6d" )
	Range ( "first_order_calc >= 0" )
	Null ( "-1" )
	Description ( "options used in first-order processing" )
	;

Attribute format
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "format" )
	Detail {
		This field lists the format of the data stored in 
		each external file.
	}
	;

Attribute geod_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "program and version used for distance calculations" )
	;

Attribute gps_discipline_mode
	Integer (10)
	Format ( "%10d" )
	Null ( "-9999" )
	Description ( "GPS discipline mode" )
	Detail {
		This is the Receiver GPS discipline mode 
	}
	;

Attribute gps_receive_mode
	Integer (10)
	Format ( "%10d" )
	Null ( "-9999" )
	Description ( "GPS receive mode" )
	Detail {
		This is the Receiver GPS receive mode
	}
	;

Attribute lat
	Real (12)
	Format ( "%12.6lf" )
	Null ( "-999.000000" )
	Range ( "lat >= -90.0 && lat <= 90.0" )
	Units ( "Degrees" )
	Description ( "estimated latitude" )
	Detail {
		This attribute is the geographic latitude.  Locations
		north of the equator have positive latitudes.
	}
	;

Attribute lddate
	Time (17)
	Format ( "%17.5lf" )
	Null ( "-9999999999.99900" )
	Units ( "Seconds" )
	Description ( "(epoch) time of last record modification" )
	Detail {
		This is the last time the record was modified.
	}
	;

Attribute lluv_tblsubtype
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "LLUV table subtype" )
	;

Attribute lluvspec_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "LLUV file specification version" )
	;

Attribute lon
	Real (12)
	Format ( "%12.6lf" )
	Null ( "-999.000000" )
	Range ( "lon >= -180.0 && lon <= 180.0" )
	Units ( "Degrees" )
	Description ( "estimated longitude" )
	Detail {
		This attribute is the geographic longitude in degrees. 
		Longitudes are measured positive east of the Greenwich
		meridian.
	}
	;

