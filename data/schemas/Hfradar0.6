	Real (10)
	Format ( "%10.4f" )
	Range ( "rad_bragg_peak_dropoff >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	Detail {
		This value is used on cross spectra to determine 
		the Bragg first-order region
	}
	;

Attribute rad_bragg_peak_null
	Real (12)
	Format ( "%12.8f" )
	Range ( "rad_bragg_peak_null >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	;

Attribute rad_merger_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of radialMerger tool used" )
	;

Attribute rad_range
	Real (15)
	Format ( "%15.6f" )
	Null ( "-9999.0" )
	Range ( "rad_range >= 0" )
	Description ( "maximum radial range" )
	Detail {
		This is the maximum radial range calculated by where
		the number of radial vectors drops to below 20% of the
		average vectors per range
	}
	;

Attribute rad_slider_ver
	String (16)
	Format ( "%-16s" )
