Schema Hfradar0.6
	Description ( "Prototype schema for HFRADAR data" )
	Detail {
		This schema holds intermediate and processed 
		measurements for Doppler-radar current measurements. 
		Support is included to track the raw files of 
		radial HFRADAR information.
	}
	Timedate lddate
	;

Attribute algorithm
	String (15)
	Format ( "%-15s" )
	Null ( "-" )
	Description ( "processing algorithm used" )
	Detail {
		This is a brief code string identifying the algorithm 
		used for processing.
	}
	;

Attribute auth
	String (15)
	Format ( "%-15s" )
	Null ( "-" )
	Description ( "source/originator of data" )
	Detail {
		This records the originator of a piece of processed 
		data, e.g. a total vector file. Note that this field 
		should be used only to identify the author, not to 
		imply a certain processing algorithm, which should be 
		separately reflected in the algorithm field.
	}
	;

Attribute bragg_smooth_pts
	Integer (4)
	Format ( "%4d" )
	Range ( "bragg_smooth_pts >= 0" )
	Null ( "-1" )
	Description ( "Number of smoothing points applied to cross spectra" )
	Detail {
		This is the number of smoothing points applied to cross 
		spectra for first-order region determination
	}
	;

Attribute cfreq
	Real (15)
	Format ( "%15.11f" )
	Null ( "-89999999999999" )
	Units ( "MHz" )
	Description ( "Center Frequency of a radar installation" )
	Detail {
		This field lists the center-frequency in MHz of a
		radar installation.
	}
	;

Attribute codartools_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of codartools.pm Perl module used" )
	;

Attribute ctf_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "CODAR/columnar table format version" )
	;

Attribute curr_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of currents tools used in OS9" )
	;

Attribute dfile
	String (64)
	Format ( "%-64s" )
	Null ( "-" )
	Description ( "data file" )
	Detail {
		This attribute is the file name of a disk-based 
		external file. See dir.
	}
	;

Attribute dir
	String (128)
	Format ( "%-128s" )
	Null ( "-" )
	Description ( "directory" )
	Detail {
		This attribute is the directory-part of a path name.
		Relative path names or '.' (dot), the notation for the
		current directory, may be used.
	}
	;

Attribute dres
	Real (12) 
	Format ( "%12.8f" )
	Range ( "dres >= 0" )
	Null ( "-9999.0" )
	Description ( "doppler cell spacing of the cross spectra" )
	;

Attribute earth_radius
	Real (21)
	Format ( "%21.15f" )
	Range ( "earth_radius >= 0" )
	Null ( "-9999.0" )
	Description ( "equatorial earth radius" )
	;

Attribute ellip
	String (50)
	Format ( "%-50s" )
	Null ( "-" )
	Description ( "ellipsoid model used to represent earth's surface" )
	;

Attribute ellip_flatten
	Real (24)
	Format ( "%24.18f" )
	Range ( "ellip_flatten >= 0" )
	Null ( "-9999.0" )
	Description ( "ellipsoid flattening term" )
	;

Attribute endtime
	Time (17)
	Format ( "%17.5f" )
	Null ( "9999999999.99900" )
	Units ( "seconds" )
	Description ( "end of a time range" )
	Detail {
		This is the time a site-table entry expires.
	}
	;

Attribute first_order_calc
	Integer (6)
	Format ( "%6d" )
	Range ( "first_order_calc >= 0" )
	Null ( "-1" )
	Description ( "options used in first-order processing" )
	;

Attribute format
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "format" )
	Detail {
		This field lists the format of the data stored in 
		each external file.
	}
	;

Attribute geod_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "program and version used for distance calculations" )
	;

Attribute lat
	Real (12)
	Format ( "%12.6lf" )
	Null ( "-999.000000" )
	Range ( "lat >= -90.0 && lat <= 90.0" )
	Units ( "Degrees" )
	Description ( "estimated latitude" )
	Detail {
		This attribute is the geographic latitude.  Locations
		north of the equator have positive latitudes.
	}
	;

Attribute lddate
	Time (17)
	Format ( "%17.5lf" )
	Null ( "-9999999999.99900" )
	Units ( "Seconds" )
	Description ( "(epoch) time of last record modification" )
	Detail {
		This is the last time the record was modified.
	}
	;

Attribute lluv_tblsubtype
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "LLUV table subtype" )
	;

Attribute lluvspec_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "LLUV file specification version" )
	;

Attribute lon
	Real (12)
	Format ( "%12.6lf" )
	Null ( "-999.000000" )
	Range ( "lon >= -180.0 && lon <= 180.0" )
	Units ( "Degrees" )
	Description ( "estimated longitude" )
	Detail {
		This attribute is the geographic longitude in degrees. 
		Longitudes are measured positive east of the Greenwich
		meridian.
	}
	;

Attribute loop1_amp_calc
	Real (15)
	Format ( "%15.8f" )
	Null ( "-9999.0" )
	Range ( "loop1_amp_calc >= 0" )
	Description ( "amplitude calculated for loop 1" )
	Detail {
		This is the amplitude calculated for loop 1 by radial 
		processing from bragg in last cross spectral file
	}
	;

Attribute loop1_phase_calc
	Real (15)
	Format ( "%15.8f" )
	Null ( "-9999.0" )
	Description ( "phase calculated for loop 1" )
	Detail {
		This is the phase calculated for loop 1 by radial 
		processing from bragg in last cross spectral file
	}
	;

Attribute loop1_amp_corr
	Real (12)
	Format ( "%12.8f" )
	Range ( "loop1_amp_corr >= 0" )
	Null ( "-9999.0" )
	Description ( "loop 1 amplitude adjustment factor" )
	;

Attribute loop2_amp_corr
	Real (12)
	Format ( "%12.8f" )
	Range ( "loop2_amp_corr >= 0" )
	Null ( "-9999.0" )
	Description ( "loop 2 amplitude adjustment factor" )
	;

Attribute loop1_phase_corr
	Real (10)
	Format ( "%10.4f" )
	Null ( "-9999.0" )
	Description ( "phase used by radial processing to adjust loop 1" )
	;

Attribute loop2_phase_corr
	Real (10)
	Format ( "%10.4f" )
	Null ( "-9999.0" )
	Description ( "phase used by radial processing to adjust loop 2" )
	;

Attribute loop2_amp_calc
	Real (15)
	Format ( "%15.8f" )
	Null ( "-9999.0" )
	Description ( "amplitude calculated for loop 2" )
	Detail {
		This is the amplitude calculated for loop 2 by radial 
		processing from bragg in last cross spectral file
	}
	;

Attribute loop2_phase_calc
	Real (15)
	Format ( "%15.8f" )
	Null ( "-9999.0" )
	Range ( "loop2_phase_calc >= 0" )
	Description ( "phase calculated for loop 2" )
	Detail {
		This is the phase calculated for loop 2 by radial 
		processing from bragg in last cross spectral file
	}
	;

Attribute manufacturer
	String (50)
	Format ( "%-50s" )
	Null ( "-" )
	Description ( "radar manufacturer" )
	;

Attribute max_curr_lim
	Real (15)
	Format ( "%15.8f" )
	Range ( "max_curr_lim >= 0" )
	Null ( "-9999.0" )
	Description ( "maximum value filter applied to radial data output" )
	;

Attribute merge_method
	Integer (4)
	Format ( "%4d" )
	Range ( "merge_method >= 0" )
	Null ( "-1" )
	Description ( "radial merging method" )
	Detail {
		This is the radial merging method for vectors falling 
		in the same range/bearing cell across short time 
		radial list
	}
	;

Attribute min_rad_vect_pts
	Integer (6)
	Format ( "%6d" )
	Range ( "min_rad_vect_pts >= 0" )
	Null ( "-1" )
	Description ( "minimum number of vectors for output" )
	Detail {
		This is the minimum number of vectors required from 
		short-time radials for output in the merged radial 
		file. 
	}
	;

Attribute mtime
	Time (17)
	Format ( "%17.5f" )
	Null ( "-9999999.99999" )
	Units ( "seconds" )
	Description ( "file modification time" )
	Detail {
		This attribute records the time of last file modification, 
		according to the filesystem.
	}
	;

Attribute music_param_01
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 1" )
	;

Attribute music_param_02
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 2" )
	;

Attribute music_param_03
	Real (12)
	Format ( "%12.8f" )
	Null ( "-9999.0" )
	Description ( "music parameter 3" )
	;

Attribute net
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "unique network identifier" )
	Detail {
		This character string is the name of a radar network.
	}
	;

Attribute netname
	String (80)
	Format ( "%-80s" )
	Null ( "-" )
	Description ( "network name" )
	Detail {
		String containing the name of a network."
	}
	;

Attribute nmerge_rads
	Integer (6)
	Format ( "%6d" )
	Null ( "-1" )
	Range ( "nmerge_rads >= 0" )
	Description ( "number of short time radials merged into radial file" )
	;

Attribute nrads
	Integer (10)
	Format ( "%10d" )
	Null ( "-1" )
	Range ( "nrads >= 0" )
	Description ( "Number of radial vectors" )
	;

Attribute patt_date
	Time (17)
	Format ( "%17.5f" )
	Null ( "-9999999999.99900" )
	Description ( "file modification date for beampattern" )
	Detail {
		This field gives the file modification date for 
		the beampattern used in radial processing
	}
	;

Attribute patt_method
	Integer (4)
	Format ( "%4d" )
	Range ( "patt_method >= 0" )
	Null ( "-1" )
	Description ( "options used by SpectraToRadial" )
	Detail {
		This field indicates options used by SpectraToRadial 
		for handling the pattern and angseg settings
	}
	;

Attribute patt_res
	Real (9)
	Format ( "%9.4f" )
	Range ( "patt_res >= 0" )
	Null ( "-9999.0" )
	Description ( "resolution of beam pattern used in radial processing" )
	;

Attribute patt_smooth
	Real (9)
	Format ( "%9.4f" )
	Range ( "patt_smooth >= 0" )
	Null ( "-9999.0" )
	Description ( "degrees smoothing used in pattern file" )
	;

Attribute patterntype
	String (2)
	Format ( "%-2s" )
	Null ( "-" )
	Description ( "beam pattern for radial files" )
	Detail {
		This field lists a single-character code for the 
		type of beam-pattern used in generating a radial 
		file. For example, 's' indicates an ideal pattern
		processed from CSS files; 'z' a measured pattern 
		from CSS files; and 'p' a measured pattern from a 
		CSA file.
	}
	;

Attribute proc_by
	String (50)
	Format ( "%-50s" )
	Null ( "-" )
	Description ( "processing center identification" )
	;

Attribute proc_time
	Time (17)
	Format ( "%17.5f" )
	Null ( "-9999999999.99900" )
	Description ( "number of short time radials merged into radial file" )
	;

Attribute proj
	String (20)
	Format ( "%-20s" )
	Null ( "-" )
	Description ( "projection" )
	Detail {
		This is the projection used for total vector
		data, usually abbreviated e.g. "eqcyl" for 
		"equidistant cylindrical"
	}
	;

Attribute rad_archiver_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of radialArchiver program" )
	;

Attribute rad_bragg_noise_thr
	Real (12)
	Format ( "%12.8f" )
	Range ( "rad_bragg_noise_thr >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	;

Attribute rad_bragg_peak_dropoff
	Real (10)
	Format ( "%10.4f" )
	Range ( "rad_bragg_peak_dropoff >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	Detail {
		This value is used on cross spectra to determine 
		the Bragg first-order region
	}
	;

Attribute rad_bragg_peak_null
	Real (12)
	Format ( "%12.8f" )
	Range ( "rad_bragg_peak_null >= 0" )
	Null ( "-9999.0" )
	Description ( "used to determine Bragg first-order region" )
	;

Attribute rad_merger_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of radialMerger tool used" )
	;

Attribute rad_slider_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of radialSlider program" )
	;

Attribute range_bin_end
	Integer (6)
	Format ( "%6d" )
	Null ( "-1" )
	Range ( "range_bin_end >= 0" )
	Description ( "last cross spectra range cell used" )
	;

Attribute range_bin_start
	Integer (6)
	Format ( "%6d" )
	Null ( "-1" )
	Range ( "range_bin_start >= 0" )
	Description ( "first cross spectra range cell used" )
	;

Attribute range_res
	Real (8)
	Format ( "%8.4f" )
	Range ( "range_res >= 0" )
	Null ( "-9999.0" )
	Description ( "distance in km between range cells" )
	;

Attribute ref_bearing
	Real (8)
	Format ( "%8.4f" )
	Range ( "ref_bearing >= 0 && ref_bearing < 360" )
	Null ( "-9999.0" )
	Description ( "direction clockwise from North" )
	Detail {
		This is the direction clockwise from north, 
		starting with subtype RDL5
	}
	;

Attribute regdescrip
	String (80)
	Format ( "%-80" )
	Description ( "full name of a region" )
	Detail {
		This field gives a human-readable description of 
		a geographic region of interest.
	}
	;

Attribute regname
	String (20)
	Format ( "%-20" )
	Description ( "short descriptive name of a region" )
	Detail {
		This field gives a short nickname for a 
		geographic region of interest.
	}
	;

Attribute res_km
	Real (10)
	Format ( "%10.5f" )
	Null ( "-1.00000" )
	Units ( "km" )
	Description ( "data resolution in km" )
	Detail {
		This field gives the resolution of total vector
		data in kilometers.
	}
	;

Attribute sampling_period_hrs
	Real (8)
	Format ( "%8.4f" )
	Null ( "-9999.0" )
	Range ( "sampling_period_hrs >= 0" )
	Units ( "hours" )
	Description ( "time coverage in hours of radial data" )
	;

Attribute second_order_bragg
	Integer (6)
	Format ( "%6d" )
	Range ( "second_order_bragg >= 0 && second_order_bragg <= 9" )
	Null ( "-1" )
	Description ( "whether cross spectra contains 2nd order" )
	Detail {
		This field is used to indicate whether to expect the 
		cross spectra to contain second-order
	}
	;

Attribute spec2rad_ver
	String (16)
	Format ( "%-16s" )
	Null ( "-" )
	Description ( "version of spectraToRadial tool used" )
	;

Attribute spec_doppler_cells
	Integer (10)
	Format ( "%10d" )
	Range ( "spec_doppler_cells >= 0" )
	Null ( "-1" )
	Description ( "number of doppler cells in the last cross spectra" )
	;

Attribute spec_range_cells
	Integer (6)
	Format ( "%6d" )
	Range ( "spec_range_cells >= 0" )
	Null ( "-1" )
	Description ( "number of range cells in the last cross spectra" )
	;

Attribute sta
	String (25)
	Format ( "%-25s" )
	Null ( "-" )
	Description ( "character-code name for a radar installation or processing center" )
	Detail {
		This field provides a unique character-code name for 
		an individual radar site, or for a coordinate-system 
		center for vector data.
	}
	;

Attribute staname
	String (50)
	Format ( "%-50s" )
	Null ( "-" )
	Description ( "full name for a radar installation" )
	Detail {
		This field provides a human-readable string name for
		an individual radar site.
	}
	;

Attribute time
	Time (17)
	Format ( "%17.5f" )
	Null ( "-9999999.99999" )
	Units ( "seconds" )
	Description ( "time" )
	Detail {
		This is the time of a measurement value, or the 
		time at which a site-table entry becomes valid. For 
		the radial measurements, this field specifies the 
		center point of the time range used in averaging 
		component cross-spectral data values.
	}
	;

Attribute xmit_bandwidth
	Real (15)
	Format ( "%15.8f" )
	Null ( "-9999.0" )
	Description ( "cross spectra bandwidth" )
	Detail {
		This is the cross spectra bandwidth. Negative values
		indicate a downward sweep. 
	}
	;

Attribute xmit_sweep_rate
	Real (12)
	Format ( "%12.8f" )
	Range ( "xmit_sweep_rate >= 0" )
	Null ( "-9999.0" )
	Description ( "cross spectra sweep rate" )
	;

Relation network
	Fields ( net netname lddate )
	Primary ( net )
	Description ( "Network description and identification" )
	Detail {
		This relation gives general information about station
		networks.  See affiliation.
	}
	;

Relation radialfiles
	Fields ( net sta time dir dfile mtime format patterntype sampling_period_hrs nmerge_rads proc_time range_bin_start range_bin_end loop1_amp_calc loop2_amp_calc loop1_phase_calc loop2_phase_calc nrads lddate )
	Primary ( net sta time format patterntype )
	Alternate ( dir dfile )
	Description ( "External files holding HFRADAR Radial data" )
	Detail {
		The radialfiles table tracks the locations of external
		files holding raw HFRADAR radial data, as well as some 
		frequently changing metadata.
	}
	;

Relation radialmeta
	Fields ( net sta time format patterntype endtime cfreq range_res ref_bearing dres manufacturer xmit_sweep_rate xmit_bandwidth max_curr_lim min_rad_vect_pts loop1_amp_corr loop2_amp_corr loop1_phase_corr loop2_phase_corr bragg_smooth_pts rad_bragg_peak_dropoff second_order_bragg rad_bragg_peak_null rad_bragg_noise_thr music_param_01 music_param_02 music_param_03 ellip earth_radius ellip_flatten rad_merger_ver spec2rad_ver ctf_ver lluvspec_ver geod_ver rad_slider_ver rad_archiver_ver patt_date patt_res patt_smooth spec_range_cells spec_doppler_cells curr_ver codartools_ver first_order_calc lluv_tblsubtype proc_by merge_method patt_method lddate )
	Primary ( net sta format patterntype time::endtime )
	Description ( "Metadata for files holding HFRADAR Radial data" )
	Detail {
		The radialmeta table tracks metadata extracted from 
		raw HFRADAR radial data.
	}
	;

Relation regnames
	Fields ( regname regdescrip lddate )
	Primary ( regname )
	Description ( "Processing regions" )
	Detail {
		This table translates processing region names into their 
		human-readable descriptions.
	}
	;

Relation site
	Fields ( net sta lat lon time endtime staname lddate )
	Primary ( net sta time::endtime )
	Defines sta
	Description ( "locations of radar sites" )
	Detail {
		This field records the locations and center frequencies 
		of radar sites (statype 'r'), together with the time ranges 
		for which those values are valid. The table also records 
		the locations of coordinate centers for vector-data 
		processing (statype 'p').
	}
	;

Relation totalfiles
	Fields ( regname time format res_km proj algorithm dir dfile mtime auth lddate )
	Primary ( regname time format res_km proj algorithm )
	Alternate ( dir dfile )
	Description ( "External files holding HFRADAR Vector data" )
	Detail {
		The vectorfiles table tracks the locations of external
		files holding processed HFRADAR vector data.
	}
	;
