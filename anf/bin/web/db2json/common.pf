    six_hrs         &Arr{
        hexadecimal    &ref(common, MASSRECENTERS_ARR{200_plus{hexadecimal}})
        value    &lt;\ 6hrs
    }
    twelve_hrs      &Arr{
        hexadecimal    &ref(common, MASSRECENTERS_ARR{100_to_199{hexadecimal}})
