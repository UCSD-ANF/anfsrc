antelope	&env(ANTELOPE)
capabilities	&Arr{
    boost	0
    matlab	1
    perl_DBI	0
    php	0
    python	1
    python_amqplib	0
    python_matplotlib	0
    python_pygraphviz	0
    vtk	0
    xmotif	1
}
macros	&Arr{
    BOOSTINCLUDE
    BOOSTLIB
    MATLAB	/opt/matlab/matlab-2009a
    PHP_EXECUTABLE
    PHP_INC
    PYTHONINCLUDE	&antelope/../python2.7.2
    PYTHON_EXECUTABLE	&antelope/bin/python
    VTKINCLUDE
    VTKLIB
    VTKROOT
    XMOTIFINCLUDE	/usr/openwin/include
    XMOTIFLIB	/usr/openwin/lib
}
