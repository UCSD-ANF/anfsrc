# css3.0 extension

Include css3.0  # uncomment to complete schema

Attribute media
    String (15)
    Format ( "%-15s" )
    Null ( "-" )
    Description ( "media in use on baler" )
    Detail {
        Media the baler is using to store data.
    }
    ;
Attribute replace
    String (1)
    Format ( "%1s" )
    Null ( "-" )
    Range ( "replace =~ /y|n/" ) 
    Description ( "replace file on local archive Y/N" )
    Detail {
        replace file on local archive with new copy Y/N.
    }
    ;
Attribute avoid
    String (1)
    Format ( "%1s" )
    Null ( "-" )
    Range ( "avoid =~ /y|n/" ) 
    Description ( "Avoid download of file Y/N" )
    Detail {
        Avoid download of file Y/N.
    }
    ;
Attribute fixed
    String (1)
    Format ( "%1s" )
    Null ( "-" )
    Range ( "fixed =~ /y|n/" ) 
    Description ( "miniSEED file header fixed Y/N" )
    Detail {
        miniSEED file header fixed Y/N.
    }
    ;
Attribute attempts
    Integer (4)
    Format ( "%4d" )
    Null ( "0" )
    Range ( "attempts > -1" )
    Description ( "Number of download attempts" )
    Detail {
        Number of download attempts for this file.
    }
    ;
Attribute mseeddb
    String (1)
    Format ( "%1s" )
    Null ( "-" )
    Range ( "mseeddb =~ /y|n/" ) 
    Description ( "miniSEED database creation Y/N" )
    Detail {
        Status of miniSEED database conversion for this file. 
    }
    ;
Attribute status
    String (15)
    Format ( "%-15s" )
    Null ( "-" )
    Description ( "file status" )
    Detail {
        Status of mseed file on baler. Downloaded, missing or error. 
    }
    ;
Attribute bandwidth
    Real (6)
    Format ( "%6.1f" )
    Null ( "-" )
    Units ( "Kb/s" )
    Description ( "bandwidth" )
    Detail {
        Calculated bandwidth of link based on total time of 
        download and size of file downloaded.
    }
    ;

Relation rsyncbaler
	Fields ( dlsta net sta dfile time endtime dir status attempts mseeddb bandwidth media fixed avoid replace )
	Primary ( dlsta dfile )
	Description ( "Track files downloaded from balers." )
	;
