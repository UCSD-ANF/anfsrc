			    }
			}
		    }
		}
	    }
	}
    }

    if ($cnt>0)
    {
	$wspd=sqrt(($ev*1.0/$cnt)**2+($nv*1.0/$cnt)**2);
	$wdir=(180.0/$pi)*atan2($ev,$nv);
	if ($wdir < 0)
	{
	    $wdir+=360;
	}
	printf FOO "$net\t$sta\t1HrAvgWSpd\t%0.4f\t%0.1f\t$cnt\n",$date_now,$wspd;
	printf FOO "$net\t$sta\t1HrAvgWDir\t%0.4f\t%0.1f\t$cnt\n",$date_now,$wdir;
