    print "\t</div>\n\n</body>\n</html>" ;
}
elsif ($in{"mode"} eq "process")
{
    ($orbname,$orbport)=split /:/, $in{"orbname"};

    $srcnameip=$orbname;
    $srcnameip=inet_aton($srcnameip);
    $srcnameip=gethostbyaddr($srcnameip, AF_INET);
    if ($srcnameip ne "")
    { $orbname2=$srcnameip; }

    foreach $portname (`egrep \"\s*$orbport\s*\" $ENV{ANTELOPE}/data/pf/orbserver_names.pf`)
    {
	if ($portname =~ /\s+$orbport\s+/)
	{
	    ($orbport2,$mold)=split /\s+/,$portname;
	}
    }

    $clientid=$in{"clientid"};

    $n=`$ENV{ANTELOPE}/bin/dbsubset $registrydb.clients "serveraddress=='$orbname' && serverport==$orbport && clientid=='$clientid'" | $ENV{ANTELOPE}/bin/dbselect -F : - written read started latency_sec select reject clientaddress perm what`;
    chomp($n);
    $n=~s/^\s+//;
    ($write,$read,$start,$latency,$select,$reject,$add,$perm,$what)=split /:/, $n, 9;
    if ($add !~ /127\.0\.0\.1/)
    {
	$srcnameip=$add;
	$srcnameip=inet_aton($srcnameip);
	$srcnameip=gethostbyaddr($srcnameip, AF_INET);
	if ($srcnameip ne "")
	{ $add2=$srcnameip; }
    }
    else
    {
	$add2=$orbname2;
    }

    print "<table class=\"tableclientinfo\">\n" ;
    print "\t<tr>\n\t\t<td class=\"tblhead\">Running on:</td>\n\t\t<td class=\"tblcont\">$add2</td>\n\t</tr>\n";
    print "\t<tr>\n\t\t<td class=\"tblhead\">Process Name:</td>\n\t\t<td class=\"tblcont\">$what</td>\n\t</tr>\n";
    print "</table>\n\n" ;
    print "<p>\&nbsp;</p>";
    print "<table class=\"tableclientinfo\">\n" ;
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Name:</td>\n\t\t<td class=\"tblcont\">$orbname2:$orbport2</td>\n\t</tr>\n";
    $mod="write mode";
    if ($perm eq "r")
    {
	$mod="read mode";
    }
    printf "\t<tr>\n\t\t<td class=\"tblhead\">ORB Data:</td>\n\t\t<td class=\"tblcont\">read: %.2f MB write: %.2f MB $mod</td>\n\t</tr>\n",$read/1024.0/1024.0,$write/1024.0/1024.0;
    if ($select =~ /^\s+$/)
    {	$select=".*";}
    if ($reject =~ /^\s+$/)
    {	$reject="None";}
    printf "\t<tr>\n\t\t<td class=\"tblhead\">ORB Select:</td>\n\t\t<td class=\"tblcont\">/$select/</td>\n\t</tr>\n";
    printf "\t<tr>\n\t\t<td class=\"tblhead\">ORB Reject:</td>\n\t\t<td class=\"tblcont\">/$reject/</td>\n\t</tr>\n";
    $latency=strtdelta($latency);
    $start=strtdelta(time()-$start);
    print "\t<tr>\n\t\t<td class=\"tblhead\">Process timing:</td>\n\t\t<td class=\"tblcont\">Latency: $latency, duration: $start</td>\n\t</tr>\n";
    print "</table>\n" ;
    ($thisapp,$c)=split /\s+/, $what;
    $thisapp =~ s/.*\///g;
    print "<p>\&nbsp;</p>" ;
    print "<table class=\"tableclientinfo\">\n" ;
    print "\t<tr>\n\t\t<th colspan=\"4\" class=\"tblhead\">ORBs running \"$thisapp\" software:</th>\n\t</tr>\n";
    foreach $i (`$ENV{ANTELOPE}/bin/dbsubset $registrydb.clients \"what=~/.*$thisapp.*/\" | $ENV{ANTELOPE}/bin/dbselect -F : - serveraddress serverport latency_sec what`)
    {
	($sadd,$sport,$lat,$what) = split /:/, $i, 4;
	$sadd =~ s/^\s+//;
	$sport =~ s/^\s+//;
	$sadd =~ s/\s+$//;
	$what =~ s/^\s+//;
	$what =~ s/\s+$//;
	$sport =~ s/\s+$//;
	$sadd2=$sadd;
	$sport2=$sport;
	$srcnameip=$sadd;
	$srcnameip=inet_aton($srcnameip);
	$srcnameip=gethostbyaddr($srcnameip, AF_INET);
	if ($srcnameip ne "")
	{ $sadd2=$srcnameip; }
	
	foreach $portname (`egrep \"\s*$sport\s*\" $ENV{ANTELOPE}/data/pf/orbserver_names.pf`)
	{
	    if ($portname =~ /\s+$sport\s+/)
	    {
		($sport2,$mold)=split /\s+/,$portname;
	    }
	}
	$lat=strtdelta($lat);
	print "\t<tr>\n\t\t<td class=\"tblcont\">*</td>\n\t\t<td class=\"tblcont\"><a href=\"orbtopo_detail.cgi?mode=orb&orbname=$sadd:$sport&registrydb=$registrydb_extern\">$sadd2:$sport2</a></td>\n\t\t<td class=\"tblcont\">$lat</td>\n\t\t<td class=\"tblcont\">$what</td>\n\t</tr>\n";
    }
    print "</table>\n\n";
    print "<p>&lt;<a href=\"orbtopo.cgi\">Orbtopo Home</a>&gt; &lt;<a href=\"http://roadnet.ucsd.edu/\">ROADNet Home</a>&gt;</p>";
    print "</div>\n" ;
    print "<div class=\"clearer\">&nbsp;</div>\n" ;
    print "</div>\n\n" ;
    print "<div id=\"footer\">\n\t<p align=\"center\">\n\t" ;
    print "\tPrototype by Todd Hansen, <a href=\"http://www.lindquistconsulting.com/\">Kent Lindquist</a> " ;
    print "\tand <a href=\"http://eqinfo.ucsd.edu/~rnewman/\">Rob Newman</a>.\n" ;
    print "\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n" ;
    print "\t<a href=\"http://roadnet.ucsd.edu\">ROADNet</a> | \n" ;
    print "\t<a href=\"http://www.sio.ucsd.edu\">SIO</a> | \n" ;
    print "\t<a href=\"http://www.ucsd.edu\">UCSD</a> \n" ;
    print "\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n" ;
    print "\tMade with <a href=\"http://www.w3.org/MarkUp/\">XHTML</a> and <a href=\"http://www.w3.org/Style/CSS/\">CSS</a>.\n" ;
    print "\t</p>" ;
    print "\t</div>\n\n</body>\n</html>" ;
    print "</body></html>\n";
