if ($in{"mode"} eq "orb")
{
    ($orbname,$orbport)=split /:/, $in{"orbname"};

    $orbname2=$orbname;
    $srcnameip=$orbname;
    $srcnameip=inet_aton($srcnameip);
    $srcnameip=gethostbyaddr($srcnameip, AF_INET);
    if ($srcnameip ne "")
    { $orbname2=$srcnameip; }

    $orbport2=$orbport;
    foreach $portname (`egrep \"\s*$orbport\s*\" $ENV{ANTELOPE}/data/pf/orbserver_names.pf`)
    {
	if ($portname =~ /\s+$orbport\s+/)
	{
	    ($orbport2,$mold)=split /\s+/,$portname;
	}
    }

    $n=`$ENV{ANTELOPE}/bin/dbsubset $registrydb.servers "serveraddress=='$orbname' && serverport==$orbport" | $ENV{ANTELOPE}/bin/dbselect - host started orb_start nclients nsources maxdata when version`;
    ($host,$started,$orb_start,$nclients,$nsources,$maxdata,$when,$version)=split /\s+/, $n, 8;
    print "<h2>Server Info</h2>\n";
    $when = strtime($when);
    print "<table class=\"tableserverinfo\">\n" ;
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Name:</td>\n\t\t<td class=\"tblcont\">$orbname2:$orbport2 (valid as of $when UTC)</td>\n\t</tr>\n" ;
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Location:</td>\n\t\t<td class=\"tblcont\">$host</td>\n\t</tr>\n" ;
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Software:</td>\n\t\t<td class=\"tblcont\">$version</td>\n\t</tr>\n";
    $started=strtdelta(time()-$started);
    $orb_start=strtdelta(time()-$orb_start);
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Uptime:</td>\n\t\t<td class=\"tblcont\">$started (last initalized $orb_start)</td>\n\t</tr>\n";
    $maxdata=sprintf("%.2f MB",$maxdata/1024.0/1024.0);
    print "\t<tr>\n\t\t<td class=\"tblhead\">ORB Stats:</td>\n\t\t<td class=\"tblcont\">$nsources sources, $nclients clients, $maxdata buffer size</td>\n\t</tr>\n";
    print "\t<tr>\n\t\t<td class=\"tblhead\">More Info:</td>\n\t\t<td class=\"tblcont\"><a href=\"#client\">Clients</a> <a href=\"#sources\">Sources</a></td>\n\t</tr>\n" ;
    print "</table>\n\n" ;
    print "<hr/>\n\n<h2>Clients</h2>\n\n";

    open(FOO,"> /tmp/orbtopo.$$.dot");
    print FOO "Digraph orbtopo {\n\trankdir=\"LR\";\n";
    print FOO "\t\"$orbname2:$orbport2\" [shape=hexagon,style=filled,fillcolor=$orbcolor]\n";
    foreach $i (`$ENV{ANTELOPE}/bin/dbsubset $registrydb.clients "serveraddress=='$orbname' && serverport==$orbport" | $ENV{ANTELOPE}/bin/dbjoin -o - $registrydb.connections | $ENV{ANTELOPE}/bin/dbjoin -o - $registrydb.databases | $ENV{ANTELOPE}/bin/dbselect - perm latency_sec fromaddress fromport toaddress toport dir dbmachine dfile closeorb clientid what`)
    {
	chomp($i);
	$i=~s/^\s+//;
	($perm,$latency,$fadd,$fport,$tadd,$tport,$dir,$dbmachine,$dfile,$closeorb,$clientid,$what)=split /\s+/, $i, 12;
	chomp($what);
	$latency=strtdelta($latency);
	($what,$c2)=split /\s+/, $what,2;
	($whatdir,$whatdfile,$whatsuffix)=parsepath($what);
	$what = $whatdfile;
	print FOO "\t\"$clientid\" [shape=$clientshape,style=filled,fillcolor=$clientcolor, label=\"$what\", href=\"orbtopo_detail.cgi?mode=process&clientid=$clientid&orbname=$orbname:$orbport&match=$match&registrydb=$registrydb_extern\"]\n";
	if ($perm =~ /r/)
	{
	    print FOO "\t\"$orbname2:$orbport2\"->\"$clientid\" [color=$orb2clientcolor, label=\"latency: $latency\"]\n";
	}
	else
	{
	    print FOO "\t\"$clientid\"->\"$orbname2:$orbport2\" [color=blue, label=\"latency: $latency\"]\n";
	}

	if ($dfile !~ /^-/)
	{
	    print FOO "\t\"$dfile\" [shape=$dbshape,style=filled,fillcolor=$dbcolor, label=\"$dfile\", href=\"orbtopo_detail.cgi?mode=database&clientid=$clientid&match=$match&registrydb=$registrydb_extern\"]\n";
	    print FOO "\t\"$clientid\"->\"$dfile\" [color=$orb2dbcolor]\n";
	}
    }
    print FOO "}\n";
    close(FOO);

    `$dot -Tpng -o $web_tempdir_filesystem/orbtopo.$$.png /tmp/orbtopo.$$.dot 2>/dev/null`;
    $map=`$dot -Tcmapx -o /dev/stdout /tmp/orbtopo.$$.dot 2>/dev/null`;
    
    print "<a name=\"clients\"><img src=\"/$web_tempdir_relative/orbtopo.$$.png\" border=\"0\" usemap=\"#orbtopo\" ismap alt=\"Orb Clients\" />\n$map\n";
    
    if ($match ne "")
    {
	print "<hr/>\n<a name=\"sources\"><h2>Sources matching regex$match2</h2>\n";
    }
    else
    {
	print "<hr/>\n<a name=\"sources\"><h2>All Sources in $orbname2:$orbport2</h2>\n";
    }

    print "<table class=\"tablesourceinfo\">\n\t<tr>\n\t\t<th class=\"tblhead\">Source Name</th>\n\t\t<th class=\"tblhead\">Latency</th>\n\t\t<th class=\"tblhead\">Duration</th>\n\t\t<th class=\"tblhead\">Size</th>\n\t</tr>\n";
    $scnt=0;
    $matchsearch="";
    if ($match ne "")
    {
	$matchsearch=" && srcname =~ /$match/";
    }
    foreach $i (`$ENV{ANTELOPE}/bin/dbsubset $registrydb.sources "serveraddress=='$orbname' && serverport==$orbport$matchsearch" | $ENV{ANTELOPE}/bin/dbselect - srcname latency_sec soldest_time slatest_time nbytes`)
    {
	($srcname,$latency,$sold,$snew,$bytes)=split /\s+/, $i;
	$latency=strtdelta($latency);
	$stime=strtdelta($snew-$sold);
	$bytes=$bytes/1024.0/1024.0;
	printf "\t<tr>\n\t\t<td class=\"tblcont\"><a href=\"orbtopo.cgi?match=$srcname&registrydb=$registrydb_extern\">$srcname</a></td>\n\t\t<td class=\"tblcont\">$latency</td>\n\t\t<td class=\"tblcont\">$stime</td>\n\t\t<td class=\"tblcont\">%.2f MB</td>\n\t</tr>\n", $bytes;
	$scnt++;
    }
    print "\t<tr>\n\t\t<th colspan=\"4\" class=\"tblhead\">$scnt sources found.</th>\n\t</tr>\n" ;
    print "</table>\n" ;
    print "<div class=\"clearer\">&nbsp;</div>\n" ;
    print "</div>\n\n" ;
    print "<div id=\"footer\">\n\t<p align=\"center\">\n\t" ;
    print "\tPrototype by Todd Hansen, <a href=\"http://www.lindquistconsulting.com/\">Kent Lindquist</a> " ;
    print "\tand <a href=\"http://eqinfo.ucsd.edu/~rnewman/\">Rob Newman</a>.\n" ;
    print "\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n" ;
    print "\t<a href=\"http://roadnet.ucsd.edu\">ROADNet</a> | \n" ;
    print "\t<a href=\"http://www.sio.ucsd.edu\">SIO</a> | \n" ;
    print "\t<a href=\"http://www.ucsd.edu\">UCSD</a> \n" ;
    print "\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n" ;
    print "\tMade with <a href=\"http://www.w3.org/MarkUp/\">XHTML</a> and <a href=\"http://www.w3.org/Style/CSS/\">CSS</a>.\n" ;
    print "\t</p>" ;
    print "\t</div>\n\n</body>\n</html>" ;
    unlink("/tmp/orbtopo.$$.dot");
    

}
